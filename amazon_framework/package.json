{
  "name": "amazon-testing",
  "version": "1.0.0",
  "description": "framework for testing amazon.com features",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "start:server": "npx webdriver-manager start",
    "update:server": "npx webdriver-manager update --versions.chrome 91.0.4472.114",
    "lint": "eslint ./*.js",
    "test": "protractor cucumberConf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://git.epam.com/Maksim_Parcheuski/test.git"
  },
  "keywords": [
    "amazon",
    "testing",
    "framework"
  ],
  "author": "Rysard Poplavskij, Anastasiya Utsiuryna, Yauheniya Latysh",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@cucumber/cucumber": "^7.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.30.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-protractor": "^2.1.1",
    "husky": "^7.0.1",
    "selenium-standalone": "^7.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-sorted": "^0.2.0",
    "cucumber-html-reporter": "^5.4.0",
    "cucumber-pretty": "^6.0.0",
    "dotenv": "^10.0.0",
    "fs-extra": "^8.1.0",
    "moment": "^2.29.1",
    "protractor": "^7.0.0",
    "protractor-cucumber-framework": "^8.2.1",
    "protractor-html-reporter": "^1.3.2",
    "selenium-webdriver": "^3.6.0",
    "webdriver-manager": "^12.1.8"
  }
}
